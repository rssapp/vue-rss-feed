{
  "name": "vue-rss-feed",
  "version": "0.2.4",
  "description": "A simple component to embed beautiful RSS feeds with Vuejs",
  "keywords": [
    "RSS",
    "Embed",
    "RSS Feeds",
    "VueJS",
    "News Feed"
  ],
  "license": "MIT",
  "homepage": "https://rss.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/RSSapp/vue-rss-feed"
  },
  "bugs": {
    "url": "https://github.com/RSSapp/vue-rss-feed/issues"
  },
  "private": false,
  "author": {
    "name": "Max S."
  },
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve ./src/demo/demo.js",
    "build-demo": "vue-cli-service build ./src/demo/demo.js --dest docs",
    "build": "vue-cli-service build --target lib --name VueRssFeed ./src/VueRssFeed.vue",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "element-ui": "^2.5.2",
    "rss-parser": "^3.6.2",
    "vue": "^2.5.22"
  }
}
